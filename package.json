{
    "name": "selenium-mocha-digital-skola",
    "version": "1.0.0",
    "description": "Selenium WebDriver tests using Mocha with Mochawesome reports",
    
    
    "scripts": {
        "test": "mocha test/sesi-9/test-sauce9.js --recursive --timeout 60000",
        "test-hw10": "mocha test/sesi-10/test-hw10.js --recursive --timeout 60000",
        "test-hw10-hook": "mocha test/sesi-10/test-hw10-hook.js --recursive --timeout 60000",
        "test-parallel": "mocha test --parallel --recursive --timeout 60000",
        "test-pertama": "mocha test --grep 'Pertama' -i --recursive --timeout 60000",
        "running-mochaawesome": "mocha test --recursive --timeout 60000 --reporter mochawesome",
        "running-simple-html": "mocha test --recursive --timeout 60000 --reporter mocha-simple-html-reporter --reporter-options output=report.html"

      
   },
   "dependencies": {
    "selenium-webdriver": "^4.17.0"
   },
   "devDependencies": {
    "mocha": "latest",
    "mochawesome": "^7.1.3",
    "mocha-simple-html-reporter": "^2.0.0" 
   }  
}
   
 
